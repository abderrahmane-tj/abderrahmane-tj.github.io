{
  "name": "tahri.dev",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "deploy": "yarn build-site && gh-pages -d dist -b master",
    "move-assets": "cp -R static dist",
    "config-custom-site": "cp CNAME dist",
    "start": "parcel serve --dist-dir serve-dist src/*.html",
    "parcel-build": "parcel build -d dist src/*.html",
    "build-site": "yarn parcel-build && yarn move-assets && yarn config-custom-site"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.0.1",
    "@parcel/transformer-webmanifest": "^2.0.1",
    "@types/lodash": "^4.14.176",
    "parcel": "^2.0.1",
    "prettier": "^2.4.1"
  }
}
